# Android SDK Build Tools Directory
# Example MacOS: ~/Library/Android/sdk/build-tools/34.0.0
# Example Linux: ~/Android/Sdk/build-tools/34.0.0
# Example Windows: C:\Users\YourName\AppData\Local\Android\Sdk\build-tools\34.0.0
ANDROID_TOOLS_DIR="/path/to/android/sdk/build-tools/34.0.0"

# Java Development Kit Home
# Example MacOS (Android Studio JBR): /Applications/Android Studio.app/Contents/jbr/Contents/Home
# Example MacOS (OpenJDK): /Library/Java/JavaVirtualMachines/openjdk-17.jdk/Contents/Home
# Example Linux: /usr/lib/jvm/java-17-openjdk
# Example Windows: C:\Program Files\Java\jdk-17
JAVA_HOME="/path/to/jdk-17"

# Solana RPC URL (Devnet for testing, Mainnet for production)
SOLANA_RPC_URL="https://api.devnet.solana.com"
# For Mainnet, use: https://api.mainnet-beta.solana.com
# Or private RPC (recommended): https://mainnet.helius-rpc.com/?api-key=fdbb7eed-e64e-49af-841e-d3fb96b97129

# Solana Keypair Path (DO NOT COMMIT THE ACTUAL KEYPAIR!)
SOLANA_KEYPAIR_PATH="./keypair.json"

# Priority Fee (in lamports) - default is 500000
PRIORITY_FEE_LAMPORTS=500000
